{
  "name": "@t1-org/t1components",
  "version": "4.4.0",
  "description": "Universal React Component Library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css",
    "./dist/index.js",
    "./dist/index.cjs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "style": "./dist/index.css",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/index.css",
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --noEmit && vite build",
    "postbuild": "npm run verify-build",
    "verify-build": "ls -la dist/ && echo 'âœ… Build files:' && find dist -name '*.css' -o -name '*.js' -o -name '*.d.ts' | sort",
    "build:watch": "vite build --watch",
    "types": "tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "dev": "vite build --watch",
    "link:local": "npm link",
    "unlink:local": "npm unlink"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "react": ">=16.8.0 || >=17.0.0 || >=18.0.0",
    "react-dom": ">=16.8.0 || >=17.0.0 || >=18.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "@mui/icons-material": {
      "optional": true
    },
    "react-router-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@mui/system": "^7.2.0",
    "@mui/x-date-pickers": "^8.8.0",
    "@types/node": "^24.0.14",
    "@types/react": "^18.3.23",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.7",
    "typescript": "^5.8.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-svgr": "^4.3.0"
  },
  "dependencies": {
    "@xyflow/react": "^12.8.2",
    "clsx": "^2.1.1",
    "country-flag-icons": "^1.5.19",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-hook-form": "^7.60.0",
    "tailwind-merge": "^2.3.0"
  }
}
